{
   "name": "@persepolis/database",
   "version": "1.0.8",
   "description": "Database schema for the Persepolis e-commerce platform.",
   "scripts": {
      "yarn": "yarn",
      "db:generate": "npx prisma generate",
      "db:format": "npx prisma format",
      "db:reset": "npx prisma db push --force-reset",
      "db:push": "npx prisma db push",
      "db:seed": "npx prisma db seed",
      "db:studio": "npx prisma studio",
      "publish": "npm publish --access public",
      "update": "npx npm-check-updates -u"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Accretence/next-prisma-tailwind-ecommerce.git"
   },
   "files": [
      "prisma"
   ],
   "author": "@accretence",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/Accretence/next-prisma-tailwind-ecommerce/issues"
   },
   "homepage": "https://github.com/Accretence/next-prisma-tailwind-ecommerce#readme",
   "devDependencies": {
      "@prisma/client": "^5.3.1",
      "@types/node": "^20.6.3",
      "prisma": "^5.3.1",
      "typescript": "^5.2.2"
   },
   "prisma": {
      "seed": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
   }
}
