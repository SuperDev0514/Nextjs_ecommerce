{
    "name": "create-next-dashboard",
    "description": "Set up a full-stack Next.js dashboard by running one NPX command.",
    "author": "@accretence",
    "license": "MIT",
    "version": "0.1.4",
    "bin": "scripts/npx.js",
    "keywords": [
        "geist-ui",
        "nextjs",
        "react",
        "serverless",
        "mongoose",
        "npx"
    ],
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "stop": "taskkill -F -IM node.exe",
        "update": "npx npm-check-updates -u",
        "data:import": "node scripts/seeder",
        "data:destroy": "node scripts/seeder -d"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Accretence/create-next-dashboard.git"
    },
    "bugs": {
        "url": "https://github.com/Accretence/create-next-dashboard/issues"
    },
    "homepage": "https://github.com/Accretence/create-next-dashboard#readme",
    "dependencies": {
        "@geist-ui/core": "^2.3.8",
        "@geist-ui/icons": "^1.0.2",
        "angra": "^0.4.6",
        "apadana": "^0.0.4",
        "axios": "^0.27.2",
        "bcryptjs": "^2.4.3",
        "cookie": "^0.5.0",
        "date-fns": "^2.29.2",
        "dotenv": "^16.0.2",
        "inter-ui": "^3.19.3",
        "jose": "^4.9.2",
        "mongoose": "^6.5.4",
        "next": "^12.2.5",
        "react": "^18.2.0",
        "react-usestateref": "^1.0.8"
    },
    "devDependencies": {
        "eslint": "^8.23.0",
        "eslint-config-next": "^12.2.5"
    }
}
